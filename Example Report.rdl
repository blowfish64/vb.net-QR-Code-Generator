      <Query>
        <DataSourceName>SoftlinkDataSource</DataSourceName>
        <CommandText>= "SELECT l3_Client.*, l3_Branch.l3_Term AS l3_BranchName, l3_ClientGroup.l3_Name AS l3_ClientGroup, l3_File.l3_FileData AS l3_FileData
 FROM l3_Client
 LEFT JOIN l3_Branch
 ON l3_Branch.l3_Uuid=l3_Client.l3_BranchUuid
 LEFT JOIN l3_ClientGroup
 ON l3_ClientGroup.l3_Uuid=l3_Client.l3_ClientGroupUuid
 LEFT JOIN l3_File
 ON l3_File.l3_Uuid = l3_Client.l3_ImageFileUuid
 WHERE l3_Client.l3_Uuid IN (" &amp; Parameters!search.Value &amp; ")
 ORDER BY l3_Client.l3_FamilyName"</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
